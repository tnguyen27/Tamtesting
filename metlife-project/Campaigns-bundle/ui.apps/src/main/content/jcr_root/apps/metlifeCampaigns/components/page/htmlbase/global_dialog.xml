<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Dialog"
    title="Global Properties"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <header
                jcr:primaryType="cq:Panel"
                title="Header">
                <items jcr:primaryType="cq:WidgetCollection">
                    <contact
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Contact"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <activeContact
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Active Contact"
                                name="./activeContact"
                                type="checkbox"
                                xtype="selection"/>
                        </items>
                    </contact>
                </items>
            </header>
            <google-analytics
                jcr:primaryType="cq:Panel"
                title="Google Analytics">
                <items jcr:primaryType="cq:WidgetCollection">
                    <apply
                        jcr:primaryType="cq:Widget"
                        allowBlank="true"
                        fieldLabel="Apply"
                        name="./apply"
                        type="checkbox"
                        xtype="selection"/>
                    <trackingId
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        anchor="95%"
                        fieldDescription="Add the UA-XXXXX-Y Key"
                        fieldLabel="Tracking ID"
                        name="./trackingId"
                        xtype="textfield"/>
                </items>
            </google-analytics>
            <vendor-scripts
                jcr:mixinTypes="[rep:AccessControllable]"
                jcr:primaryType="cq:Widget"
                path="/apps/metlifeCommons/components/page/htmlbase/tab_vendorscripts.infinity.json"
                xtype="cqinclude"/>
        </items>
    </items>
</jcr:root>
