<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Page Titles Configuration"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <pagetitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Page Title"
                        name="./pagetitle"
                        xtype="textfield"/>
                    <pagenumberPhone
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Example 1-844-629-7794"
                        fieldLabel="Number Phone"
                        name="./pagenumberPhone"
                        xtype="textfield"/>
                    <pagesubtitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Page SubTitle"
                        name="./pagesubtitle"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
