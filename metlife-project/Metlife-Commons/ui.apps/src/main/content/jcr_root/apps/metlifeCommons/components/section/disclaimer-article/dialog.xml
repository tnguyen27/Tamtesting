<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="600"
    title="Dialog Disclaimer Component"
    width="700"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <configuration
                jcr:primaryType="cq:Panel"
                title="Disclaimer Component">
                <items jcr:primaryType="cq:WidgetCollection">
                    <text
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Disclaimer Component"
                        height="500"
                        hideLabel="true"
                        name="./disclaimerComponentText"
                        xtype="richtext">
                        <rtePlugins jcr:primaryType="nt:unstructured">
                            <styles
                                jcr:primaryType="nt:unstructured"
                                features="*">
                                <styles jcr:primaryType="cq:WidgetCollection">
                                    <title-style
                                        jcr:primaryType="nt:unstructured"
                                        cssName="news-article__resources--title"
                                        text="Title Style"/>
                                    <paragraph-style
                                        jcr:primaryType="nt:unstructured"
                                        cssName="news-article__resources--copy"
                                        text="Paragraph Style"/>
                                    <disclaimer-style
                                        jcr:primaryType="nt:unstructured"
                                        cssName="disclaimer metlife-copy-9"
                                        text="Disclaimer Style"/>
                                </styles>
                            </styles>
                            <paraformat
                                jcr:primaryType="nt:unstructured"
                                features="*">
                                <formats jcr:primaryType="cq:WidgetCollection">
                                    <paragraph
                                        jcr:primaryType="nt:unstructured"
                                        description="Paragraph"
                                        tag="p"/>
                                    <heading1
                                        jcr:primaryType="nt:unstructured"
                                        description="Heading 1"
                                        tag="h1"/>
                                    <heading2
                                        jcr:primaryType="nt:unstructured"
                                        description="Heading 2"
                                        tag="h2"/>
                                    <heading3
                                        jcr:primaryType="nt:unstructured"
                                        description="Heading 3"
                                        tag="h3"/>
                                </formats>
                            </paraformat>
                            <subsuperscript
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                        </rtePlugins>
                    </text>
                </items>
            </configuration>
        </items>
    </items>
</jcr:root>
