<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Media Contact Config"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <configuration
                jcr:primaryType="cq:Panel"
                title="Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <company
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Company"
                        name="./company"
                        xtype="textfield"/>
                    <contactName
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Name"
                        name="./contactName"
                        xtype="textfield"/>
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./title"
                        xtype="textfield"/>
                    <phone
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Phone"
                        name="./phone"
                        xtype="textfield"/>
                    <email
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Email"
                        name="./email"
                        xtype="textfield"/>
                </items>
            </configuration>
        </items>
    </items>
</jcr:root>
