<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Contact Row Configuration"
    width="700"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <configuration
                jcr:primaryType="cq:Panel"
                title="Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <contacts
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Contact Configuration Paths"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <contactConfigPath1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="First"
                                name="./cnt1"
                                rootPath="/etc/metlife/contacts/media-contact"
                                xtype="pathfield"/>
                            <contactConfigPath2
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Second"
                                name="./cnt2"
                                rootPath="/etc/metlife/contacts/media-contact"
                                xtype="pathfield"/>
                            <contactConfigPath3
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Third"
                                name="./cnt3"
                                rootPath="/etc/metlife/contacts/media-contact"
                                xtype="pathfield"/>
                            <contactConfigPath4
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Fourth"
                                name="./cnt4"
                                rootPath="/etc/metlife/contacts/media-contact"
                                xtype="pathfield"/>
                        </items>
                    </contacts>
                </items>
            </configuration>
        </items>
    </items>
</jcr:root>
