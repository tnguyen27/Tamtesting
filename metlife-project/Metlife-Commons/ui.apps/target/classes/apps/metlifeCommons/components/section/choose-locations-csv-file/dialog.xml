<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <Configuration
                jcr:primaryType="cq:Panel"
                title="Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <locations-file
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Choose a CSV file containing office locations"
                        fieldLabel="Offices Location File"
                        name="./locationsFile"
                        rootPath="/content/dam/metlifecom"
                        xtype="pathfield"/>
                </items>
            </Configuration>
        </items>
    </items>
</jcr:root>
