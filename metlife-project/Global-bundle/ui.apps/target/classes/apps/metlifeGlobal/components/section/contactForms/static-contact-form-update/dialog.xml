<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    title="Static contact form personal update"
    width="700"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <configuration
                jcr:primaryType="cq:Panel"
                title="Labels">
                <items jcr:primaryType="cq:WidgetCollection">
                    <prodDetails
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Product details configurations label"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <prodDetailTitle
                                jcr:primaryType="cq:Widget"
                                defaultValue="Product Details"
                                fieldLabel="Title"
                                name="./prodDetailTitle"
                                xtype="textfield"/>
                            <prodDetailStatus
                                jcr:primaryType="cq:Widget"
                                defaultValue="(*Required)"
                                fieldLabel="Status"
                                name="./prodDetailStatus"
                                xtype="textfield"/>
                            <leadBasedOnPolicyNumber
                                jcr:primaryType="cq:Widget"
                                defaultValue="Tipo y Número de Documento"
                                fieldLabel="Policy Number"
                                name="./leadBasedOnPolicyNumber"
                                xtype="textfield"/>
                        </items>
                    </prodDetails>
                    <personalInformationReq
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Personal information required"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <personalInfoTitle
                                jcr:primaryType="cq:Widget"
                                defaultValue="Personal Information"
                                fieldLabel="Title"
                                name="./personalInfoTitle"
                                xtype="textfield"/>
                            <personalInfoStatus
                                jcr:primaryType="cq:Widget"
                                defaultValue="(*Required)"
                                fieldLabel="Status"
                                name="./personalInfoStatus"
                                xtype="textfield"/>
                            <firstName
                                jcr:primaryType="cq:Widget"
                                defaultValue="Nombre*"
                                fieldLabel="Name"
                                name="./firstName"
                                xtype="textfield"/>
                            <lastName
                                jcr:primaryType="cq:Widget"
                                defaultValue="Apellido*"
                                fieldLabel="Last name"
                                name="./lastName"
                                xtype="textfield"/>
                            <phone
                                jcr:primaryType="cq:Widget"
                                defaultValue="Telefono Particular*"
                                fieldLabel="Phone"
                                name="./phone"
                                xtype="textfield"/>
                            <mobilePhone
                                jcr:primaryType="cq:Widget"
                                defaultValue="Telefono Celular"
                                fieldLabel="Mobile Phone"
                                name="./mobilePhone"
                                xtype="textfield"/>
                            <email
                                jcr:primaryType="cq:Widget"
                                defaultValue="Dirección de email*"
                                fieldLabel="Email"
                                name="./email"
                                xtype="textfield"/>
                        </items>
                    </personalInformationReq>
                    <personalInformationOpt
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Personal information optional"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <optPersonalInfoTitle
                                jcr:primaryType="cq:Widget"
                                defaultValue="Personal Information"
                                fieldLabel="Title"
                                name="./optPersonalInfoTitle"
                                xtype="textfield"/>
                            <optPersonalInfoStatus
                                jcr:primaryType="cq:Widget"
                                defaultValue="(Optional)"
                                fieldLabel="Status"
                                name="./optPersonalInfoStatus"
                                xtype="textfield"/>
                            <address
                                jcr:primaryType="cq:Widget"
                                defaultValue="Domicilio completo de Correspondencia"
                                fieldLabel="Address"
                                name="./address"
                                xtype="textfield"/>
                            <city
                                jcr:primaryType="cq:Widget"
                                defaultValue="Localidad"
                                fieldLabel="City"
                                name="./city"
                                xtype="textfield"/>
                            <zip
                                jcr:primaryType="cq:Widget"
                                defaultValue="Zip"
                                fieldLabel="Zip"
                                name="./zip"
                                xtype="textfield"/>
                            <employerNatureOfBusiness
                                jcr:primaryType="cq:Widget"
                                defaultValue="Profesion/Cargo"
                                fieldLabel="Nature of Business"
                                name="./employerNatureOfBusiness"
                                xtype="textfield"/>
                            <employerName
                                jcr:primaryType="cq:Widget"
                                defaultValue="Empresa"
                                fieldLabel="Company Name"
                                name="./employerName"
                                xtype="textfield"/>
                        </items>
                    </personalInformationOpt>
                    <refer1Information
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="First refer information"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <ref1Title
                                jcr:primaryType="cq:Widget"
                                defaultValue="Referido 1:"
                                fieldLabel="Title"
                                name="./ref1Title"
                                xtype="textfield"/>
                            <ref1Name
                                jcr:primaryType="cq:Widget"
                                defaultValue="Nombre Completo"
                                fieldLabel="Name"
                                name="./ref1Name"
                                xtype="textfield"/>
                            <ref1Email
                                jcr:primaryType="cq:Widget"
                                defaultValue="Dirección de email*"
                                fieldLabel="Email"
                                name="./ref1Email"
                                xtype="textfield"/>
                            <ref1Phone
                                jcr:primaryType="cq:Widget"
                                defaultValue="Telefono de Contacto"
                                fieldLabel="Phone"
                                name="./ref1Phone"
                                xtype="textfield"/>
                        </items>
                    </refer1Information>
                    <refer2Information
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Second refer information"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <ref2Title
                                jcr:primaryType="cq:Widget"
                                defaultValue="Referido 2:"
                                fieldLabel="Title"
                                name="./ref2Title"
                                xtype="textfield"/>
                            <ref2Name
                                jcr:primaryType="cq:Widget"
                                defaultValue="Nombre Completo"
                                fieldLabel="Name"
                                name="./ref2Name"
                                xtype="textfield"/>
                            <ref2Email
                                jcr:primaryType="cq:Widget"
                                defaultValue="Dirección de email*"
                                fieldLabel="Email"
                                name="./ref2Email"
                                xtype="textfield"/>
                            <ref2Phone
                                jcr:primaryType="cq:Widget"
                                defaultValue="Telefono de Contacto"
                                fieldLabel="Phone"
                                name="./ref2Phone"
                                xtype="textfield"/>
                        </items>
                    </refer2Information>
                    <buttonLabel
                        jcr:primaryType="cq:Widget"
                        defaultValue="Enviar"
                        fieldLabel="Button label"
                        name="./buttonLabel"
                        xtype="textfield"/>
                </items>
            </configuration>
            <messages
                jcr:primaryType="cq:Panel"
                title="Messages">
                <items jcr:primaryType="cq:WidgetCollection">
                    <disclaimer
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Disclaimer"
                        name="./disclaimer"
                        xtype="richtext">
                        <rtePlugins jcr:primaryType="nt:unstructured">
                            <styles
                                jcr:primaryType="nt:unstructured"
                                features="*">
                                <styles jcr:primaryType="cq:WidgetCollection">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        cssName="contactDisclaimer"
                                        text="Contact disclaimer style"/>
                                </styles>
                            </styles>
                        </rtePlugins>
                    </disclaimer>
                    <thanksMessage
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Configuration for thank you messages after submitted"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <thankYouMessageTitle
                                jcr:primaryType="cq:Widget"
                                defaultValue="Your Form has been successfully submitted!"
                                fieldLabel="Title"
                                name="./thankYouMessageTitle"
                                xtype="textfield"/>
                            <thankYouMessageContent
                                jcr:primaryType="cq:Widget"
                                defaultValue="Someone from MetLife will be reaching shortly to provide you help."
                                fieldLabel="Subtitle"
                                name="./thankYouMessageContent"
                                xtype="textfield"/>
                        </items>
                    </thanksMessage>
                    <errorsMessage
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Configuration for error messages after submitted"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <errorMessageTitle
                                jcr:primaryType="cq:Widget"
                                defaultValue="Error!"
                                fieldLabel="Title"
                                name="./errorMessageTitle"
                                xtype="textfield"/>
                            <errorMessageContent
                                jcr:primaryType="cq:Widget"
                                defaultValue="An error occured while submitting your information."
                                fieldLabel="Subtitle"
                                name="./errorMessageContent"
                                xtype="textfield"/>
                        </items>
                    </errorsMessage>
                </items>
            </messages>
        </items>
    </items>
</jcr:root>
