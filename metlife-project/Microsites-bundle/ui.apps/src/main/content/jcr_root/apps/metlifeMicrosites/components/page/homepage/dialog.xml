<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:primaryType="cq:Dialog"
    height="650"
    stateful="false"
    width="800"
    xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <A_basic
                    jcr:primaryType="cq:Widget"
                    path="/apps/metlifeCommons/components/page/htmlbase/tab_basic.infinity.json"
                    xtype="cqinclude"/>
                <H_customconf
                    jcr:primaryType="cq:Widget"
                    path="/apps/metlifeMicrosites/components/page/htmlbase/customconfiguration.infinity.json"
                    xtype="cqinclude"/>
                <header
                    jcr:primaryType="cq:Panel"
                    title="Header">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <logo-metlife
                            jcr:primaryType="cq:Widget"
                            collapsed="{Boolean}false"
                            collapsible="{Boolean}true"
                            title="Logo MetLife"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <image
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="false"
                                    anchor="95%"
                                    ddGroups="[media]"
                                    fieldLabel="Image"
                                    name="./logo-metlife/image/fileReference"
                                    rootPath="/content/dam"
                                    rootTitle="Assets"
                                    xtype="pathfield"/>
                                <link
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Link"
                                    linkPattern="\{0}.html"
                                    name="./logoLinkMetLife"
                                    rootPath="/content/metlife"
                                    rootTitle="MetLife"
                                    xtype="pathfield"/>
                                <link-target
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="false"
                                    anchor="95%"
                                    defaultValue="_self"
                                    fieldLabel="Open in"
                                    key="target"
                                    name="./targetMetlife"
                                    type="select"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <self
                                            jcr:primaryType="nt:unstructured"
                                            text="Same Page"
                                            value="_self"/>
                                        <blank
                                            jcr:primaryType="nt:unstructured"
                                            text="New Page"
                                            value="_blank"/>
                                    </options>
                                </link-target>
                                <alt
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Alt Text"
                                    name="./logoAltTextMetLife"
                                    xtype="textfield"/>
                                <imageresourcetype
                                    jcr:primaryType="cq:Widget"
                                    ignoreData="true"
                                    name="./logo-metlife/image/sling:resourceType"
                                    value="foundation/components/image"
                                    xtype="hidden"/>
                            </items>
                        </logo-metlife>
                        <search
                            jcr:primaryType="cq:Widget"
                            collapsed="{Boolean}true"
                            collapsible="{Boolean}true"
                            title="Search"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <activeSearch
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Active Search"
                                    name="./activeSearch"
                                    type="checkbox"
                                    xtype="selection"/>
                                <selectsearch
                                    jcr:primaryType="cq:Widget"
                                    defaultValue="internalsearch"
                                    fieldLabel="Search type"
                                    name="./selectsearch"
                                    type="radiogroup"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <radiobutton1
                                            jcr:primaryType="cq:Widget"
                                            text="Internal Search"
                                            value="internalsearch"/>
                                        <radiobutton2
                                            jcr:primaryType="cq:Widget"
                                            text="Legacy Search"
                                            value="legacysearch"/>
                                    </options>
                                </selectsearch>
                                <label
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Label"
                                    name="./searchLabel"
                                    xtype="textfield"/>
                                <imageresourcetype
                                    jcr:primaryType="cq:Widget"
                                    ignoreData="true"
                                    name="./search/image/sling:resourceType"
                                    value="foundation/components/image"
                                    xtype="hidden"/>
                                <placeholder
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Placeholder Label"
                                    name="./searchPlaceholderLabel"
                                    xtype="textfield"/>
                                <imageformresourcetype
                                    jcr:primaryType="cq:Widget"
                                    ignoreData="true"
                                    name="./search/imageForm/sling:resourceType"
                                    value="foundation/components/image"
                                    xtype="hidden"/>
                                <link
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Results Page Link"
                                    linkPattern="\{0}.html"
                                    name="./searchLink"
                                    rootPath="/content/metlife"
                                    rootTitle="MetLife"
                                    xtype="pathfield"/>
                                <legacylink
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Legacy Results Page Link"
                                    name="./LegacyLink"
                                    xtype="textfield"/>
                            </items>
                        </search>
                        <logo-other
                            jcr:primaryType="cq:Widget"
                            collapsed="{Boolean}false"
                            collapsible="{Boolean}true"
                            title="Logo Other"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <activeLogo
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Active Other Logo"
                                    name="./activeLogoOther"
                                    type="checkbox"
                                    xtype="selection"/>
                                <image
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    ddGroups="[media]"
                                    fieldLabel="Image"
                                    name="./logo-other/image/fileReference"
                                    rootPath="/content/dam"
                                    rootTitle="Assets"
                                    xtype="pathfield"/>
                                <link
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Link"
                                    linkPattern="\{0}.html"
                                    name="./logoLinkOther"
                                    rootPath="/content/metlife"
                                    rootTitle="MetLife"
                                    xtype="pathfield"/>
                                <link-target
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="false"
                                    anchor="95%"
                                    defaultValue="_self"
                                    fieldLabel="Open in"
                                    key="target"
                                    name="./targetOther"
                                    type="select"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <self
                                            jcr:primaryType="nt:unstructured"
                                            text="Same Page"
                                            value="_self"/>
                                        <blank
                                            jcr:primaryType="nt:unstructured"
                                            text="New Page"
                                            value="_blank"/>
                                    </options>
                                </link-target>
                                <alt
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Alt Text"
                                    name="./logoAltTextOther"
                                    xtype="textfield"/>
                                <imageresourcetype
                                    jcr:primaryType="cq:Widget"
                                    ignoreData="true"
                                    name="./logo-other/image/sling:resourceType"
                                    value="foundation/components/image"
                                    xtype="hidden"/>
                            </items>
                        </logo-other>
                    </items>
                </header>
                <footer
                    jcr:primaryType="cq:Panel"
                    title="Footer">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <footerlogo
                            jcr:primaryType="cq:Widget"
                            collapsed="{Boolean}false"
                            collapsible="{Boolean}true"
                            title="Footer Logo"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <enabledLogo
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Enable Logo Footer"
                                    name="./enableLogoFooter"
                                    type="checkbox"
                                    xtype="selection"/>
                                <image
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    ddGroups="[media]"
                                    fieldLabel="Image"
                                    name="./footer/image/fileReference"
                                    rootPath="/content/dam"
                                    rootTitle="Assets"
                                    xtype="pathfield"/>
                                <alt
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Alt Text"
                                    name="./footerlogoAltText"
                                    xtype="textfield"/>
                                <logoLink
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Logo Link"
                                    linkPattern="\{0}.html"
                                    name="./footerlogoLink"
                                    rootPath="/content/metlife"
                                    rootTitle="MetLife"
                                    xtype="pathfield"/>
                                <link-target1
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    defaultValue="_self"
                                    fieldLabel="Open in"
                                    key="target"
                                    name="./targetLogo"
                                    type="select"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <self
                                            jcr:primaryType="nt:unstructured"
                                            text="Same Page"
                                            value="_self"/>
                                        <blank
                                            jcr:primaryType="nt:unstructured"
                                            text="New Page"
                                            value="_blank"/>
                                    </options>
                                </link-target1>
                                <label
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Label"
                                    name="./footerLogoLabel"
                                    xtype="textfield"/>
                                <labelLink
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldLabel="Label Logo Link"
                                    linkPattern="\{0}.html"
                                    name="./footerlabelLink"
                                    rootPath="/content/metlife"
                                    rootTitle="MetLife"
                                    xtype="pathfield"/>
                                <link-target2
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    defaultValue="_self"
                                    fieldLabel="Open in"
                                    key="target"
                                    name="./targetLabel"
                                    type="select"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <self
                                            jcr:primaryType="nt:unstructured"
                                            text="Same Page"
                                            value="_self"/>
                                        <blank
                                            jcr:primaryType="nt:unstructured"
                                            text="New Page"
                                            value="_blank"/>
                                    </options>
                                </link-target2>
                                <imageresourcetype
                                    jcr:primaryType="cq:Widget"
                                    ignoreData="true"
                                    name="./footer/image/sling:resourceType"
                                    value="foundation/components/image"
                                    xtype="hidden"/>
                            </items>
                        </footerlogo>
                        <disclaimer
                            jcr:primaryType="cq:Widget"
                            collapsed="{Boolean}false"
                            collapsible="{Boolean}true"
                            title="Disclaimer"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <hideDisclaimer
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Hide disclaimer"
                                    name="./hideDisclaimer"
                                    type="checkbox"
                                    xtype="selection"/>
                            </items>
                        </disclaimer>
                        <footer-bullets
                            jcr:primaryType="cq:Widget"
                            collapsed="{Boolean}false"
                            collapsible="{Boolean}true"
                            title="Footer Bullets"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <enabledLinks
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Enable Footer Bullets:"
                                    name="./enableBulletsFooter"
                                    type="checkbox"
                                    xtype="selection"/>
                                <bullets
                                    jcr:primaryType="cq:Widget"
                                    addItemLabel="Add Bullet"
                                    fieldLabel="Footer Bullets"
                                    hideLabel="false"
                                    name="./footerbulletlinks"
                                    xtype="multifield">
                                    <fieldConfig
                                        jcr:primaryType="cq:Widget"
                                        xtype="multifieldpanel">
                                        <items jcr:primaryType="cq:WidgetCollection">
                                            <A_title
                                                jcr:primaryType="nt:unstructured"
                                                allowBlank="false"
                                                fieldLabel="Label"
                                                key="navigationTitle"
                                                validateOnBlur="true"
                                                xtype="textfield"/>
                                            <C_pages
                                                jcr:primaryType="cq:Widget"
                                                allowBlank="false"
                                                anchor="100%"
                                                fieldLabel="URL"
                                                key="path"
                                                linkPattern="\{0}.html"
                                                rootPath="/content/metlife"
                                                xtype="pathfield"/>
                                            <D_target
                                                jcr:primaryType="cq:Widget"
                                                allowBlank="false"
                                                defaultValue="_self"
                                                fieldLabel="Open in"
                                                key="target"
                                                type="select"
                                                xtype="selection">
                                                <options jcr:primaryType="cq:WidgetCollection">
                                                    <self
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Same Page"
                                                        value="_self"/>
                                                    <blank
                                                        jcr:primaryType="nt:unstructured"
                                                        text="New Page"
                                                        value="_blank"/>
                                                </options>
                                            </D_target>
                                        </items>
                                    </fieldConfig>
                                </bullets>
                            </items>
                        </footer-bullets>
                        <legaldisclaimer
                            jcr:primaryType="cq:Widget"
                            border="{Boolean}true"
                            collapsed="{Boolean}false"
                            collapsible="{Boolean}true"
                            fieldDescription="To add the Year, add this format: xxYEARxx"
                            title="Legal disclaimer"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <enableLegalDisclaimer
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Enable Legal Disclaimer"
                                    name="./enableLegalDisclaimer"
                                    type="checkbox"
                                    xtype="selection"/>
                                <text
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldDescription="To add the Year, add this format: xxYEARxx"
                                    hideLabel="{Boolean}true"
                                    name="./footerLegalDisclaimer"
                                    removeSingleParagraphContainer="{Boolean}true"
                                    xtype="richtext">
                                    <rtePlugins jcr:primaryType="nt:unstructured">
                                        <subsuperscript
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    </rtePlugins>
                                </text>
                            </items>
                        </legaldisclaimer>
                        <peaunutdisclaimer
                            jcr:primaryType="cq:Widget"
                            border="{Boolean}true"
                            collapsed="{Boolean}true"
                            collapsible="{Boolean}true"
                            title="Peanuts disclaimer"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <enabledPeanutsDisclaimer
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Enable Peanuts Disclaimer"
                                    name="./enablePeanutsDisclaimer"
                                    type="checkbox"
                                    xtype="selection"/>
                                <peanutstext
                                    jcr:primaryType="cq:Widget"
                                    anchor="95%"
                                    fieldDescription="To add the Year, add this format: xxYEARxx"
                                    hideLabel="{Boolean}true"
                                    name="./footerPeanutsDisclaimer"
                                    removeSingleParagraphContainer="{Boolean}true"
                                    xtype="richtext">
                                    <rtePlugins jcr:primaryType="nt:unstructured">
                                        <subsuperscript
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    </rtePlugins>
                                </peanutstext>
                            </items>
                        </peaunutdisclaimer>
                        <bulletsfield
                            jcr:primaryType="nt:unstructured"
                            ignoreData="true"
                            name="./footerbulletlinks@TypeHint"
                            value="String[]"
                            xtype="hidden"/>
                    </items>
                </footer>
                <G_metatagging
                    jcr:mixinTypes="[rep:AccessControllable]"
                    jcr:primaryType="cq:Widget"
                    path="/apps/metlifeMicrosites/components/page/htmlbase/metlife_advanced.infinity.json"
                    xtype="cqinclude"/>
                <C_advanced
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_advanced.infinity.json"
                    xtype="cqinclude"/>
                <D_blueprint
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_blueprint.infinity.json"
                    xtype="cqinclude"/>
                <E_cloudservices
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_cloudservices.infinity.json"
                    xtype="cqinclude"/>
                <F_livecopy
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_livecopy.infinity.json"
                    xtype="cqinclude"/>
            </items>
        </tabs>
    </items>
</jcr:root>
