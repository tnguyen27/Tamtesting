<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Product Comparison Table"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <products
                jcr:primaryType="cq:Panel"
                title="Products">
                <items jcr:primaryType="cq:WidgetCollection">
                    <product-list
                        jcr:primaryType="cq:Widget"
                        addItemLabel="Add a Product"
                        fieldLabel="Products"
                        hideLabel="true"
                        maxItems="{Long}5"
                        name="./pathRefs"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            name="./pathRefs"
                            xtype="pathfield"/>
                    </product-list>
                </items>
            </products>
            <general
                jcr:primaryType="cq:Panel"
                title="General">
                <items jcr:primaryType="cq:WidgetCollection">
                    <coverageLastsLabel
                        jcr:primaryType="nt:unstructured"
                        defaultValue="Coverage lasts:"
                        fieldDescription="Overwrite this label."
                        fieldLabel="Coverage Lasts Label"
                        maxLength="150"
                        name="./coverageLastsLabel"
                        value="Coverage lasts:"
                        xtype="textfield"/>
                    <availableOnlineLabel
                        jcr:primaryType="nt:unstructured"
                        defaultValue="Available online:"
                        fieldDescription="Overwrite this label."
                        fieldLabel="Available Online Label"
                        maxLength="150"
                        name="./availableOnlineLabel"
                        value="Available online:"
                        xtype="textfield"/>
                    <buildCashValueLabel
                        jcr:primaryType="nt:unstructured"
                        defaultValue="Build cash value:"
                        fieldDescription="Overwrite this label."
                        fieldLabel="Build Cash Value Label"
                        maxLength="150"
                        name="./buildCashValueLabel"
                        value="Build cash value:"
                        xtype="textfield"/>
                    <medicalExamRequiredLabel
                        jcr:primaryType="nt:unstructured"
                        defaultValue="Medical exam required:"
                        fieldDescription="Overwrite this label."
                        fieldLabel="Medical Exam Required Label"
                        maxLength="150"
                        name="./medicalExamRequiredLabel"
                        value="Medical exam required:"
                        xtype="textfield"/>
                    <coverageAmountsLabel
                        jcr:primaryType="nt:unstructured"
                        defaultValue="Coverage amounts:"
                        fieldDescription="Overwrite this label.&lt;br>"
                        fieldLabel="Coverage Amounts Label"
                        maxLength="150"
                        name="./coverageAmountsLabel"
                        value="Coverage amounts:"
                        xtype="textfield"/>
                    <learnMoreButtonLabel
                        jcr:primaryType="nt:unstructured"
                        defaultValue="LEARN MORE"
                        fieldDescription="Overwrite this label."
                        fieldLabel="Learn More Button Label"
                        maxLength="150"
                        name="./learnMoreButtonLabel"
                        value="LEARN MORE"
                        xtype="textfield"/>
                </items>
            </general>
            <Disclaimer
                jcr:primaryType="cq:Widget"
                anchor="100%"
                title="Text"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <text
                        jcr:primaryType="cq:Widget"
                        hideLabel="{Boolean}true"
                        name="./text"
                        xtype="richtext">
                        <rtePlugins jcr:primaryType="nt:unstructured">
                            <subsuperscript
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                        </rtePlugins>
                    </text>
                    <isRichTextFlag
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./textIsRich"
                        value="true"
                        xtype="hidden"/>
                </items>
            </Disclaimer>
        </items>
    </items>
</jcr:root>
