<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Quote Results Configuration"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <quote-title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Quote title"
                        name="./quoteTitle"
                        xtype="textfield"/>
                    <ratesLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Rates Text"
                        name="./ratesText"
                        xtype="textfield"/>
                    <editQuoteLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Edit Quote Label"
                        name="./editQuoteLabel"
                        xtype="textfield"/>
                    <contactHeaderLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Contact Header Label"
                        name="./contactHeaderLabel"
                        xtype="textfield"/>
                    <contactBullets
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Contact Bullet points"
                        name="./contactBullets"
                        xtype="multifield"/>
                    <contactBulltesTH
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./contactBullets@TypeHint"
                        value="String[]"
                        xtype="hidden"/>
                </items>
            </tab1>
            <messages
                jcr:primaryType="cq:Panel"
                title="Messages">
                <items jcr:primaryType="cq:WidgetCollection">
                    <applyButtonDisclaimer
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Disclaimer Apply Button"
                        name="./applyButtonDisclaimer"
                        xtype="richtext"/>
                    <thanksHeaderText
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Thanks Header Text"
                        name="./thanksHeaderText"
                        xtype="textfield"/>
                    <thanksSubheaderText
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Thanks Subheader Text"
                        name="./thanksSubheaderText"
                        xtype="textfield"/>
                    <thanskContentText
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Thanks content text"
                        name="./thanksContentText"
                        xtype="textfield"/>
                    <thanksBullets
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Thanks Bullet points"
                        name="./thanksBullets"
                        xtype="multifield"/>
                    <contactBulltesId
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./thanksBullets@TypeHint"
                        value="String[]"
                        xtype="hidden"/>
                    <errorHeaderMessage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Error header message"
                        name="./errorHeaderMessage"
                        xtype="textfield"/>
                    <errorSubheaderMessage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Error subheader message"
                        name="./errorSubheaderMessage"
                        xtype="textfield"/>
                </items>
            </messages>
            <phonecontact
                jcr:primaryType="cq:Panel"
                title="Phone contact label">
                <items jcr:primaryType="cq:WidgetCollection">
                    <firstContactText
                        jcr:primaryType="cq:Widget"
                        fieldLabel="First contact text"
                        name="./firstContactText"
                        xtype="textfield"/>
                    <firstContactNumber
                        jcr:primaryType="cq:Widget"
                        fieldLabel="First contact number"
                        name="./firstContactNumber"
                        xtype="textfield"/>
                    <applyByPhoneNow
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Apply by phone"
                        name="./applyByPhoneNow"
                        xtype="textfield"/>
                    <callNowText
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Call now text"
                        name="./callNowText"
                        xtype="textfield"/>
                    <secondContactText
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Second contact label"
                        name="./secondContactText"
                        xtype="textfield"/>
                    <secondContactNumber
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Second contact number"
                        name="./secondContactNumber"
                        xtype="textfield"/>
                    <callMessage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Message for call"
                        name="./callMessage"
                        xtype="richtext"/>
                </items>
            </phonecontact>
        </items>
    </items>
</jcr:root>
