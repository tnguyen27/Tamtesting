<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Panel"
    title="Template Configuration">
    <items jcr:primaryType="cq:WidgetCollection">
        <generalConfig
            jcr:primaryType="cq:Widget"
            path="/apps/metlifeCommons/components/page/htmlbase/customconfiguration/items.infinity.json"
            xtype="cqinclude"/>
        <activateconfig
            jcr:primaryType="cq:Widget"
            collapsible="{Boolean}true"
            title="Subnavigation Configuration"
            xtype="dialogfieldset">
            <items jcr:primaryType="cq:WidgetCollection">
                <showsubnav
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Show Subnavigation"
                    name="./showSubNav"
                    type="checkbox"
                    xtype="selection"/>
            </items>
        </activateconfig>
        <sitemap
            jcr:primaryType="cq:Widget"
            collapsible="{Boolean}true"
            title="Sitemap"
            xtype="dialogfieldset">
            <items jcr:primaryType="cq:WidgetCollection">
                <changefrequency
                    jcr:primaryType="cq:Widget"
                    defaultValue="monthly"
                    fieldLabel="Change Frequency"
                    name="./changefreq"
                    type="select"
                    value="monthly"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <daily
                            jcr:primaryType="nt:unstructured"
                            text="Daily"
                            value="daily"/>
                        <weekly
                            jcr:primaryType="nt:unstructured"
                            text="Weekly"
                            value="weekly"/>
                        <monthly
                            jcr:primaryType="nt:unstructured"
                            text="Monthly"
                            value="monthly"/>
                        <annualy
                            jcr:primaryType="nt:unstructured"
                            text="Annualy"
                            value="annualy"/>
                    </options>
                </changefrequency>
                <priority
                    jcr:primaryType="cq:Widget"
                    defaultValue="0.25"
                    fieldLabel="Priority"
                    name="./priority"
                    type="select"
                    value="0.25"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <_x0031_
                            jcr:primaryType="nt:unstructured"
                            text="1"
                            value="1"/>
                        <_x0030_.75
                            jcr:primaryType="nt:unstructured"
                            text="0.75"
                            value="0.75"/>
                        <_x0030_.50
                            jcr:primaryType="nt:unstructured"
                            text="0.50"
                            value="0.50"/>
                        <_x0030_.25
                            jcr:primaryType="nt:unstructured"
                            text="0.25"
                            value="0.25"/>
                    </options>
                </priority>
            </items>
        </sitemap>
    </items>
</jcr:root>
