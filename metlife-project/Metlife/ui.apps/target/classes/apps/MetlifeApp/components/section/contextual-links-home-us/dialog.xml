<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="550"
    title="Contextual Links Home US Dialog"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="nt:unstructured"
                        anchor="95%"
                        fieldLabel="Title"
                        maxLength="40"
                        name="./titleContLinkus"
                        width="414"
                        xtype="textfield"/>
                    <contextualLinksHome
                        jcr:primaryType="cq:Widget"
                        addItemLabel="Add a new Contextual Link"
                        fieldDescription="Click the '+' to add a new link (Min:1, Max:3)"
                        fieldLabel="Contextual Links Home US"
                        hideLabel="false"
                        maxItems="{Long}3"
                        minItems="{Long}1"
                        name="./contextualLinksHome"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="multifieldpanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <image-config
                                    jcr:primaryType="cq:Widget"
                                    collapsed="{Boolean}true"
                                    collapsible="{Boolean}false"
                                    title="Image Configuration"
                                    xtype="dialogfieldset"/>
                                <image
                                    jcr:primaryType="nt:unstructured"
                                    allowBlank="false"
                                    anchor="95%"
                                    fieldLabel="Image"
                                    key="contextualLinkImageHome"
                                    name="./contextualLinkImagesHome/fileReferences"
                                    rootPath="/content/dam"
                                    validateOnBlur="true"
                                    xtype="pathfield"/>
                                <mobile-image
                                    jcr:primaryType="nt:unstructured"
                                    allowBlank="false"
                                    anchor="95%"
                                    fieldLabel="Mobile Image"
                                    key="contextualLinkMobileImageHome"
                                    name="./contextualLinkMobileImagesHome/fileReferences"
                                    rootPath="/content/dam"
                                    validateOnBlur="true"
                                    xtype="pathfield"/>
                                <description
                                    jcr:primaryType="nt:unstructured"
                                    allowBlank="false"
                                    anchor="95%"
                                    fieldLabel="Description"
                                    key="descriptionHome"
                                    width="265"
                                    xtype="textarea"/>
                                <altText
                                    jcr:primaryType="nt:unstructured"
                                    allowBlank="false"
                                    anchor="95%"
                                    fieldLabel="Alt Text"
                                    key="altTextHome"
                                    validateOnBlur="true"
                                    xtype="textfield"/>
                                <link
                                    jcr:primaryType="nt:unstructured"
                                    allowBlank="false"
                                    anchor="95%"
                                    fieldLabel="Link"
                                    key="linkHome"
                                    linkPattern="\{0}.html"
                                    rootPath="/content/metlife"
                                    rootTitle="MetLife"
                                    width="265"
                                    xtype="pathfield"/>
                                <link-target
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="false"
                                    anchor="95%"
                                    defaultValue="_self"
                                    fieldLabel="Open in"
                                    key="targetHome"
                                    type="select"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <self
                                            jcr:primaryType="nt:unstructured"
                                            text="Same Page"
                                            value="_self"/>
                                        <blank
                                            jcr:primaryType="nt:unstructured"
                                            text="New Page"
                                            value="_blank"/>
                                    </options>
                                </link-target>
                                <link-config
                                    jcr:primaryType="cq:Widget"
                                    collapsed="{Boolean}true"
                                    collapsible="{Boolean}false"
                                    title="Link Configuration"
                                    xtype="dialogfieldset"/>
                                <label-link
                                    jcr:primaryType="nt:unstructured"
                                    allowBlank="false"
                                    anchor="95%"
                                    fieldLabel="Label Link"
                                    key="labelLink"
                                    width="265"
                                    xtype="textfield"/>
                                <bottom-link
                                    jcr:primaryType="nt:unstructured"
                                    allowBlank="false"
                                    anchor="95%"
                                    fieldLabel="Bottom Link"
                                    key="bottomLink"
                                    linkPattern="\{0}.html"
                                    rootPath="/content/metlife"
                                    rootTitle="MetLife"
                                    width="265"
                                    xtype="pathfield"/>
                                <bottomLink-target
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="false"
                                    anchor="95%"
                                    defaultValue="_self"
                                    fieldLabel="Open in"
                                    key="target"
                                    type="select"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <self
                                            jcr:primaryType="nt:unstructured"
                                            text="Same Page"
                                            value="_self"/>
                                        <blank
                                            jcr:primaryType="nt:unstructured"
                                            text="New Page"
                                            value="_blank"/>
                                    </options>
                                </bottomLink-target>
                            </items>
                        </fieldConfig>
                    </contextualLinksHome>
                    <contextualLinkImagesField
                        jcr:primaryType="nt:unstructured"
                        ignoreData="true"
                        name="./contextualLinksHome@TypeHint"
                        value="String[]"
                        xtype="hidden"/>
                    <imageNode
                        jcr:primaryType="nt:unstructured"
                        ignoreData="true"
                        name="./contextualLinkImagesHome/sling:resourceType"
                        value="xumakbase/components/section/base"
                        xtype="hidden"/>
                    <mobileImageNode
                        jcr:primaryType="nt:unstructured"
                        ignoreData="true"
                        name="./contextualLinkMobileImagesHome/sling:resourceType"
                        value="xumakbase/components/section/base"
                        xtype="hidden"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
