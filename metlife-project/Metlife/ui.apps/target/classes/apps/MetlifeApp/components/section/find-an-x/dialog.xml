<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    title="Find an X"
    width="650"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <units
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        defaultValue="mi"
                        fieldLabel="Units"
                        name="./units"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <km
                                jcr:primaryType="nt:unstructured"
                                text="Kilometers"
                                value="km"/>
                            <mi
                                jcr:primaryType="nt:unstructured"
                                text="Miles"
                                value="mi"/>
                        </options>
                    </units>
                    <empty-text
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        defaultValue="Please enter your location to find an office near you."
                        fieldLabel="Empty Text"
                        name="./emptyText"
                        xtype="textfield"/>
                    <search-placeholder
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        defaultValue="Zip Code or City, State"
                        fieldLabel="Search Placeholder Text"
                        name="./searchPlaceholderText"
                        xtype="textfield"/>
                    <display-results
                        jcr:primaryType="cq:Widget"
                        addItemLabel="Add Option"
                        fieldDescription="The options for the 'displaying' dropdown. E.g. 10, 20, 30"
                        fieldLabel="Display Results Options"
                        minItems="{Long}1"
                        name="./displayResultsOptions"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="numberfield"/>
                    </display-results>
                    <results-hint
                        jcr:primaryType="nt:unstructured"
                        ignoreData="true"
                        name="./displayResultsOptions@TypeHint"
                        value="String[]"
                        xtype="hidden"/>
                    <radius-options
                        jcr:primaryType="cq:Widget"
                        addItemLabel="Add Option"
                        fieldDescription="The options for the radius dropdown. E.g. 1, 2, 10"
                        fieldLabel="Radius Options"
                        minItems="{Long}1"
                        name="./radiusOptions"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="numberfield"/>
                    </radius-options>
                    <radius-hint
                        jcr:primaryType="nt:unstructured"
                        ignoreData="true"
                        name="./radiusOptions@TypeHint"
                        value="String[]"
                        xtype="hidden"/>
                    <service
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        defaultValue="legacy"
                        fieldDescription="Select the service where the search will be done"
                        fieldLabel="Service"
                        name="./selector"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <legacy
                                jcr:primaryType="nt:unstructured"
                                text="Legacy"
                                value="legacy"/>
                            <local
                                jcr:primaryType="nt:unstructured"
                                text="Local"
                                value="local"/>
                        </options>
                    </service>
                    <localpath
                        jcr:primaryType="cq:Widget"
                        fieldDescription="This path make reference to the list of locations when &quot;Local&quot; is selected as a service."
                        fieldLabel="Local Path"
                        name="./locationsPath"
                        rootPath="etc/metlife"
                        rootTitle="Configurations"
                        xtype="pathfield"/>
                    <activeOptions
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enable/disable  Information Options"
                        name="./activeInformationOptions"
                        type="checkbox"
                        xtype="selection"/>
                    <information-options
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Click the '+' to add a new item"
                        fieldLabel="Information Options"
                        name="./informationOptions"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="multifieldpanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <option
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="false"
                                    anchor="90%"
                                    fieldLabel="Option"
                                    key="option"
                                    xtype="textfield"/>
                            </items>
                        </fieldConfig>
                    </information-options>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Displaying Labels">
                <items jcr:primaryType="cq:WidgetCollection">
                    <find
                        jcr:primaryType="cq:Widget"
                        defaultValue="Find"
                        fieldLabel="'Find' Label"
                        name="./findLabel"
                        xtype="textfield"/>
                    <displaying
                        jcr:primaryType="cq:Widget"
                        defaultValue="Displaying"
                        fieldLabel="'Displaying' Label"
                        name="./displayingLabel"
                        xtype="textfield"/>
                    <of
                        jcr:primaryType="cq:Widget"
                        defaultValue="of"
                        fieldLabel="'of' Label"
                        name="./ofLabel"
                        xtype="textfield"/>
                    <more-map
                        jcr:primaryType="cq:Widget"
                        defaultValue="More Map"
                        fieldLabel="'More map' Label"
                        name="./moreMapLabel"
                        xtype="textfield"/>
                    <phone
                        jcr:primaryType="cq:Widget"
                        defaultValue="Telephone"
                        fieldLabel="Phone Label"
                        name="./phoneLabel"
                        xtype="textfield"/>
                    <alt-phone
                        jcr:primaryType="cq:Widget"
                        defaultValue="Alternate Telephone"
                        fieldLabel="Alternate Phone Label"
                        name="./alternatePhoneLabel"
                        xtype="textfield"/>
                    <fax
                        jcr:primaryType="cq:Widget"
                        defaultValue="FAX"
                        fieldLabel="Fax Label"
                        name="./faxLabel"
                        xtype="textfield"/>
                    <email
                        jcr:primaryType="cq:Widget"
                        defaultValue="Additional Info"
                        fieldLabel="E-mail Label"
                        name="./emailLabel"
                        xtype="textfield"/>
                    <secondary-email
                        jcr:primaryType="cq:Widget"
                        defaultValue="Secondary Email"
                        fieldLabel="Secondary E-mail Label"
                        name="./secondaryEmailLabel"
                        xtype="textfield"/>
                    <main-contact
                        jcr:primaryType="cq:Widget"
                        defaultValue="Main Contact"
                        fieldLabel="Main Contact Label"
                        name="./mainContactLabel"
                        xtype="textfield"/>
                    <hours
                        jcr:primaryType="cq:Widget"
                        defaultValue="Working Hours"
                        fieldLabel="Hours Label"
                        name="./hoursLabel"
                        xtype="textfield"/>
                    <more-info
                        jcr:primaryType="cq:Widget"
                        defaultValue="Additional Info"
                        fieldLabel="Additional Info Label"
                        name="./additionalInfoLabel"
                        xtype="textfield"/>
                </items>
            </tab2>
            <hidden-properties
                jcr:primaryType="cq:Panel"
                title="Interaction Labels">
                <items jcr:primaryType="cq:WidgetCollection">
                    <education
                        jcr:primaryType="nt:unstructured"
                        allowblank="{Boolean}false"
                        defaultValue="Education"
                        fieldLabel="Education Label"
                        name="./education"
                        xtype="textfield"/>
                    <laguages
                        jcr:primaryType="nt:unstructured"
                        defaultValue="Languages"
                        fieldLabel="Languages Label"
                        name="./languages"
                        xtype="textfield"/>
                    <gender
                        jcr:primaryType="nt:unstructured"
                        defaultValue="Gender"
                        fieldLabel="Gender Label"
                        name="./gender"
                        xtype="textfield"/>
                    <get-directions-text
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        defaultValue="Get Directions"
                        fieldLabel="Get Directions Text"
                        name="./getDirectionsText"
                        xtype="textfield"/>
                    <error-message-server-busy
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        defaultValue="Sorry, please try again later."
                        fieldLabel="Error Message Server Busy"
                        name="./errorMessageServerBusy"
                        xtype="textfield"/>
                    <error-message-no-office-found
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        defaultValue="No offices found. Please expand your search area."
                        fieldLabel="Error Message No Office Found"
                        name="./errorMessageNoOfficeFound"
                        xtype="textfield"/>
                    <get-direction-more-map
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        defaultValue="More Map"
                        fieldLabel="Get Direction More Map"
                        name="./getDirectionMoreMap"
                        xtype="textfield"/>
                    <get-direction-less-map
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        defaultValue="Less Map"
                        fieldLabel="Get Direction Less Map"
                        name="./getDirectionLessMap"
                        xtype="textfield"/>
                </items>
            </hidden-properties>
            <get-directions-labels
                jcr:primaryType="cq:Panel"
                title="Get Directions Labels">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="nt:unstructured"
                        allowBlank="false"
                        anchor="98%"
                        defaultValue="Get Directions For"
                        fieldLabel="Get Directions Title"
                        name="./getDirectionsTitle"
                        xtype="textfield"/>
                    <back-button
                        jcr:primaryType="nt:unstructured"
                        allowBlank="false"
                        anchor="98%"
                        defaultValue="Back"
                        fieldLabel="Back Button Label"
                        name="./getDirectionsBackButton"
                        xtype="textfield"/>
                    <get-directions-button
                        jcr:primaryType="nt:unstructured"
                        allowBlank="false"
                        anchor="98%"
                        defaultValue="Get Directions"
                        fieldLabel="Get Directions Button Label"
                        name="./getDirectionsButton"
                        xtype="textfield"/>
                    <from
                        jcr:primaryType="nt:unstructured"
                        allowBlank="false"
                        anchor="98%"
                        defaultValue="From"
                        fieldLabel="From"
                        name="./getDirectionsFrom"
                        xtype="textfield"/>
                    <to
                        jcr:primaryType="nt:unstructured"
                        allowBlank="false"
                        anchor="98%"
                        defaultValue="To"
                        fieldLabel="To Label"
                        name="./getDirectionsTo"
                        xtype="textfield"/>
                    <unable
                        jcr:primaryType="nt:unstructured"
                        allowBlank="false"
                        anchor="98%"
                        defaultValue="Unable to find directions between the selected addresses"
                        fieldLabel="Not Found Message"
                        name="./getDirectionsUnableToFound"
                        xtype="textarea"/>
                </items>
            </get-directions-labels>
            <sideFormconfig
                jcr:primaryType="cq:Widget"
                path="/apps/MetlifeApp/components/section/static/contact-sidebar-form/dialog/items/items/configuration.infinity.json"
                xtype="cqinclude"/>
        </items>
    </items>
</jcr:root>
