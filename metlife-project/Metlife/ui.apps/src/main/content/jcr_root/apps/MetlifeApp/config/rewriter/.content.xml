<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <metlife
        jcr:primaryType="nt:unstructured"
        contentTypes="[text/html]"
        enabled="{Boolean}true"
        generatorType="htmlparser"
        order="{Long}1"
        serializerType="htmlwriter"
        transformerTypes="[linkchecker,contentsync,xumak-link-rewriter,xumak-map-rewriter]">
        <transformer-wcm-io-urlhandler-externalizer
            jcr:primaryType="nt:unstructured"
            rewriteElements="[img:src]"/>
        <generator-htmlparser
            jcr:primaryType="nt:unstructured"
            includeTags="[DIV,/DIV,IMG,A,/A,FORM,BASE,LINK,SCRIPT,BODY,/BODY,AREA]"/>
    </metlife>
</jcr:root>
