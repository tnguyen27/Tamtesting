<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Find a X Configuration"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="General">
                <items jcr:primaryType="cq:WidgetCollection">
                    <enabledComponent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enable Find an X Module"
                        name="./enableComp"
                        type="checkbox"
                        xtype="selection"/>
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./title"
                        xtype="textfield"/>
                    <buttonLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Submit Button Label"
                        name="./submitLabel"
                        xtype="textfield"/>
                    <zipError
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Message ZIP Message"
                        name="./zipError"
                        xtype="textfield"/>
                    <overlayLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Overlay Label"
                        name="./overlayLabel"
                        xtype="textfield"/>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Widget"
                title="Office">
                <items jcr:primaryType="cq:WidgetCollection">
                    <officeLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Office Label"
                        name="./officeLabel"
                        xtype="textfield"/>
                    <inputPlaceholder
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Input Placeholder"
                        name="./officePlaceholder"
                        xtype="textfield"/>
                    <link
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        fieldLabel="Link"
                        linkPattern="\{0}.html"
                        name="./officeLink"
                        rootPath="/content/metlife"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                </items>
            </tab2>
            <tab3
                jcr:primaryType="cq:Panel"
                title="Dentist">
                <items jcr:primaryType="cq:WidgetCollection">
                    <enabledComponent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enable Find a Dentist"
                        name="./enableDentist"
                        type="checkbox"
                        xtype="selection"/>
                    <dentistLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Dentist Label"
                        name="./dentistLabel"
                        xtype="textfield"/>
                    <inputPlaceholder
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Input Placeholder"
                        name="./dentistPlaceholder"
                        xtype="textfield"/>
                    <networkPlaceholder
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Network Placeholder"
                        name="./networkPlaceholder"
                        xtype="textfield"/>
                    <networkOptions
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Network Options"
                        name="./networkOptions"
                        rootPath="/etc/acs-commons/lists/metlife/fax-tool"
                        xtype="pathfield"/>
                </items>
            </tab3>
            <tab4
                jcr:primaryType="cq:Panel"
                title="Vision Provider">
                <items jcr:primaryType="cq:WidgetCollection">
                    <enabledComponent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enable Vision Provider"
                        name="./enableVision"
                        type="checkbox"
                        xtype="selection"/>
                    <visionProvdLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Vision Provider Label"
                        name="./visionProvLabel"
                        xtype="textfield"/>
                    <inputPlaceholder
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Input Placeholder"
                        name="./visionPlaceholder"
                        xtype="textfield"/>
                    <planPlaceholder
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Plan Placeholder"
                        name="./planPlaceholder"
                        xtype="textfield"/>
                    <planOptions
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Plan options"
                        name="./planOptions"
                        rootPath="/etc/acs-commons/lists/metlife/fax-tool"
                        xtype="pathfield"/>
                </items>
            </tab4>
        </items>
    </items>
</jcr:root>
