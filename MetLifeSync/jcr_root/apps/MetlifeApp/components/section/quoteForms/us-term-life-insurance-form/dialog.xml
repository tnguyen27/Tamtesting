<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <stateGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="State"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <statePlaceholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="State Placeholder"
                                name="./stateplaceholder"
                                xtype="textfield"/>
                            <stateSelect
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Select the list to be used on the field."
                                fieldLabeld="State List"
                                name="./stateList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <goLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Go Label"
                                name="./goLabel"
                                xtype="textfield"/>
                            <ErrorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Error Label"
                                name="./errorStateLabel"
                                xtype="textfield"/>
                        </items>
                    </stateGroup>
                    <dateGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Date"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <monthPlaceholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Month Placeholder"
                                name="./monthPlaceholder"
                                xtype="textfield"/>
                            <monthSelect
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Select the list to be used on the field."
                                fieldLabeld="Month List"
                                name="./monthList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <dayPlaceholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Day Placeholder"
                                name="./dayPlaceholder"
                                xtype="textfield"/>
                            <daySelect
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Select the list to be used on the field."
                                fieldLabeld="Day List"
                                name="./daySelect"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <yearPlaceholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Year Placeholder"
                                name="./yearPlaceholder"
                                xtype="textfield"/>
                            <yearSelect
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Select the list to be used on the field."
                                fieldLabeld="Year List"
                                name="./yearList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <ErrorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Date Error Label"
                                name="./dateErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </dateGroup>
                    <genderGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Gender"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <Placeholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placeholder"
                                name="./genderPlaceholder"
                                xtype="textfield"/>
                            <Select
                                jcr:primaryType="cq:Widget"
                                fieldDescription="Select the list to be used on the field."
                                fieldLabel="Gender List"
                                name="./genderList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <ErrorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Error Label"
                                name="./genderErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </genderGroup>
                    <coverageGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Coverage Amount"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <cvPlaceholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placeholder"
                                name="./coveragePlaceholder"
                                xtype="textfield"/>
                            <cvSelect
                                jcr:primaryType="cq:Widget"
                                fieldDescription="Select the list to be used on the field."
                                fieldLabel="Coverage List"
                                name="./coverageList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <cvErrorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Error Label"
                                name="./coverageErrorLabel"
                                xtype="textfield"/>
                            <calculatorLink
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Calculator link"
                                name="./calculatorLink"
                                rootPath="/content/metlife"
                                xtype="pathfield"/>
                        </items>
                    </coverageGroup>
                    <termGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Term Length"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <placeholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Term Length Placeholder"
                                name="./termGroupPlaceholder"
                                xtype="textfield"/>
                            <select
                                jcr:primaryType="cq:Widget"
                                fieldDescription="Select the list to be used on the field."
                                fieldLabel="Term Length List"
                                fieldLabeld="Benefit List"
                                name="./termList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <errorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Term Error Label"
                                name="./termErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </termGroup>
                    <tobaccoGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Tobacco Use"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <placeholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Tobacco Use Placeholder"
                                name="./tobacoPlaceholder"
                                xtype="textfield"/>
                            <select
                                jcr:primaryType="cq:Widget"
                                fieldDescription="Select the list to be used on the field."
                                fieldLabel="Tobacco UseList"
                                name="./tobaccoList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <errorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Tobaco Use Error Label"
                                name="./tobaccoErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </tobaccoGroup>
                    <healthGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Your Health"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <placeholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Health Placeholder"
                                name="./healthPlaceholder"
                                xtype="textfield"/>
                            <select
                                jcr:primaryType="cq:Widget"
                                fieldDescription="Select the list to be used on the field."
                                fieldLabel="Health List"
                                name="./healthList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <errorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Health Error Label"
                                name="./healtErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </healthGroup>
                    <platformGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Platform"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <onlineLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Online Label"
                                name="./onlineLabel"
                                xtype="textfield"/>
                            <withRepLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="With a Rep Label"
                                name="./wrepLabel"
                                xtype="textfield"/>
                            <ErrorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Error Label"
                                name="./wayFormErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </platformGroup>
                    <dataproduct
                        jcr:primaryType="cq:Widget"
                        defaultValue="us_term"
                        name="./dataproduct"
                        value="us_term"
                        xtype="hidden"/>
                    <pathtoresults
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        anchor="95%"
                        fieldLabel="Path to results"
                        linkPattern="\{0}.html"
                        maxLength="40"
                        name="./submit_btn_link"
                        rootPath="/content/metlife"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
