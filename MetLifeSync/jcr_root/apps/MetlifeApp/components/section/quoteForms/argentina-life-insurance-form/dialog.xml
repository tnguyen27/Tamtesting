<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Argentina Life Insurance Form"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Fields">
                <items jcr:primaryType="cq:WidgetCollection">
                    <genderOptions
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Gender"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <select
                                jcr:primaryType="cq:Widget"
                                fieldDescription="Select the list to be used on the field."
                                fieldLabel="List"
                                name="./genderList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <Placeholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Age Placeholder"
                                name="./genderPlaceholder"
                                xtype="textfield"/>
                            <ErrorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Error Label"
                                name="./genderErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </genderOptions>
                    <ageGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Age"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <Placeholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Age Placeholder"
                                name="./agePlaceholder"
                                xtype="textfield"/>
                            <select
                                jcr:primaryType="cq:Widget"
                                fieldDescription="Select the list to be used on the field."
                                fieldLabel="List"
                                name="./ageList"
                                rootPath="/etc/acs-commons/lists/metlife/quote-tool"
                                xtype="pathfield"/>
                            <errorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Error Label"
                                name="./ageErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </ageGroup>
                    <coverageGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Coverage Amount"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <aiPlaceholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placeholder"
                                name="./coveragePlaceholder"
                                xtype="textfield"/>
                            <aiErrorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Error Label"
                                name="./coverageErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </coverageGroup>
                    <termyearGroup
                        jcr:primaryType="cq:Widget"
                        collapsed="true"
                        collapsible="true"
                        title="Term Years"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <aiPlaceholder
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placeholder"
                                name="./termyearsPlaceholder"
                                xtype="textfield"/>
                            <aiErrorLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Error Label"
                                name="./termyearsErrorLabel"
                                xtype="textfield"/>
                        </items>
                    </termyearGroup>
                    <dataproduct
                        jcr:primaryType="cq:Widget"
                        defaultValue="arg_termLife"
                        name="./dataproduct"
                        value="arg_termLife"
                        xtype="hidden"/>
                    <pathtoresults
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        anchor="95%"
                        fieldLabel="Path to results"
                        linkPattern="\{0}.html"
                        name="./submit_btn_link"
                        rootPath="/content/metlife"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
