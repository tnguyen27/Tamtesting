---
# General Configuration
# ---
# url: http://localhost:4502
url: <%= @aem_url %>
# user: admin:admin
user: <%= @aem_user %>:<%= @aem_password %>
# dir: /opt/day/CQ/author/crx-quickstart
dir: <%= @install_dir %>/crx-quickstart
# The OS user in charge of running CQ
#systemuser: cq
systemuser: <%= @aem_os_user %>

# Online Backup Configuration
# ---
# This installDir must be the path to crx-quickstart's parent directory.
#installDir: /opt/day/CQ/author
installDir: <%= @install_dir %>
#target: /opt/day/CQ/backup
target: <%= @aem_backup_directory %>
#delay: 1
delay: <%= @aem_backup_delay %>
