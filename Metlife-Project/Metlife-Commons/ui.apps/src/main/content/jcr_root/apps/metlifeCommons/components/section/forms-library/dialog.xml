<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="600"
    title="Forms Library"
    width="950"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <basic
                jcr:primaryType="cq:Panel"
                title="Basic">
                <items jcr:primaryType="cq:WidgetCollection">
                    <downloadText
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Download text"
                        name="./downloadText"
                        xtype="textfield"/>
                    <form-path
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Forms directory"
                        name="./formsPath"
                        rootPath="/content/dam/metlifecom"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                    <filter
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Filter"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <label
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Label"
                                name="./filterLabel"
                                xtype="textfield"/>
                            <default-value
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Default value"
                                name="./filterValue"
                                xtype="textfield"/>
                        </items>
                    </filter>
                    <pagination
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Pagination"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <previous
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Previous Label"
                                name="./previousLabel"
                                xtype="textfield"/>
                            <next
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Next Label"
                                name="./nextLabel"
                                xtype="textfield"/>
                        </items>
                    </pagination>
                    <default-category
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Default Category"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <title
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Title"
                                name="./defaultCategoryTitle"
                                xtype="textfield"/>
                        </items>
                    </default-category>
                    <disclaimer
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Disclaimer"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <text
                                jcr:primaryType="cq:Widget"
                                hideLabel="{Boolean}true"
                                name="./formsLibraryDisclaimer"
                                xtype="richtext">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <formats jcr:primaryType="cq:WidgetCollection">
                                            <paragraph
                                                jcr:primaryType="nt:unstructured"
                                                description="Paragraph"
                                                tag="p"/>
                                            <heading1
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 1"
                                                tag="h1"/>
                                            <heading2
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 2"
                                                tag="h2"/>
                                            <heading3
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 3"
                                                tag="h3"/>
                                            <heading4
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 4"
                                                tag="h4"/>
                                            <heading5
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 5"
                                                tag="h5"/>
                                            <heading6
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 6"
                                                tag="h6"/>
                                        </formats>
                                    </paraformat>
                                    <subsuperscript
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <styles
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <styles jcr:primaryType="cq:WidgetCollection">
                                            <disclaimer
                                                jcr:primaryType="nt:unstructured"
                                                cssName="brand-gray-2nd hidden-xs"
                                                text="Forms Library Disclaimer"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                cssName="section-title__title"
                                                text="Forms Library Disclaimer Title"/>
                                        </styles>
                                    </styles>
                                </rtePlugins>
                            </text>
                        </items>
                    </disclaimer>
                </items>
            </basic>
            <default-files
                jcr:primaryType="cq:Panel"
                title="Default forms">
                <items jcr:primaryType="cq:WidgetCollection">
                    <file-one
                        jcr:primaryType="cq:Widget"
                        fieldLabel="File 1"
                        name="./file1"
                        regex="/.(pdf|doc|docx|xls|xlsx)$/i"
                        rootPath="/content/dam/metlifecom"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                    <file-two
                        jcr:primaryType="cq:Widget"
                        fieldLabel="File 2"
                        name="./file2"
                        regex="/.(pdf|doc|docx|xls|xlsx)$/i"
                        rootPath="/content/dam/metlifecom"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                    <file-three
                        jcr:primaryType="cq:Widget"
                        fieldLabel="File 3"
                        name="./file3"
                        regex="/.(pdf|doc|docx|xls|xlsx)$/i"
                        rootPath="/content/dam/metlifecom"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                    <file-four
                        jcr:primaryType="cq:Widget"
                        fieldLabel="File 4"
                        name="./file4"
                        regex="/.(pdf|doc|docx|xls|xlsx)$/i"
                        rootPath="/content/dam/metlifecom"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                    <file-five
                        jcr:primaryType="cq:Widget"
                        fieldLabel="File 5"
                        name="./file5"
                        regex="/.(pdf|doc|docx|xls|xlsx)$/i"
                        rootPath="/content/dam/metlifecom"
                        rootTitle="MetLife"
                        xtype="pathfield"/>
                </items>
            </default-files>
        </items>
    </items>
</jcr:root>
