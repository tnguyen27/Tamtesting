                                    fieldDescription="Click the '+' to add a new link"
                                    fieldLabel="Links"
                                    key="links"
                                    maxItems="{Long}7"
                                    minItems="{Long}1"
                                    xtype="multifield">
                                    <fieldConfig
                                        jcr:primaryType="cq:Widget"
                                        xtype="multifieldpanel">
                                        <items jcr:primaryType="cq:WidgetCollection">
                                            <linkText
                                                jcr:primaryType="nt:unstructured"
                                                allowBlank="false"
                                                anchor="98%"
                                                fieldLabel="Link text"
                                                key="linkText"
                                                validateOnBlur="true"
                                                xtype="textfield"/>
                                            <link
                                                jcr:primaryType="cq:Widget"
                                                allowBlank="false"
                                                anchor="98%"
                                                fieldLabel="Link"
                                                key="link"
                                                linkPattern="\{0}.html"
                                                xtype="pathfield"/>
                                            <linkTarget
                                                jcr:primaryType="cq:Widget"
                                                allowBlank="false"
                                                anchor="98%"
                                                fieldLabel="Open in"
                                                key="target"
                                                type="select"
                                                xtype="selection">
                                                <options jcr:primaryType="cq:WidgetCollection">
                                                    <self
                                                        jcr:primaryType="nt:unstructured"
                                                        text="same window"
                                                        value="_self"/>
                                                    <newWindow
                                                        jcr:primaryType="nt:unstructured"
                                                        text="new window"
                                                        value="_blank"/>
                                                </options>
                                            </linkTarget>
                                        </items>
                                    </fieldConfig>
                                </links>
                            </items>
                        </fieldConfig>
                    </familyLinks3>
                </items>
            </thirdcolumn>
        </items>
    </items>
</jcr:root>
