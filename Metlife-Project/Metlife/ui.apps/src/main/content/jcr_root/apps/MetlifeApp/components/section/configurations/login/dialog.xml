                        jcr:primaryType="cq:Widget"
                        fieldLabel="Activate account type"
                        name="./activateLoginType"
                        type="checkbox"
                        xtype="selection">
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            loadcontent="function(field,record,path){Metlife.Components.LoginAccount.loadContent(field,record,path);}"
                            selectionchanged="function(selection, value, isChecked) { Metlife.Components.LoginAccount.selectionChanged(selection, value, isChecked);}"/>
                    </activateLoginType>
                    <individuals
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        itemId="individuals"
                        title="For individuals tooltip text"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <individualCopyFirst
                                jcr:primaryType="cq:Widget"
                                defaultValue="MetLife's Online Service - Life, Annuities, Disability, Long-Term Care, Critical Illness, Auto, Home, Total Control Account (eSERVICE)"
